{
  "version": 2,
  "devCommand": "npm run dev",
  "builds": [
      {
          "src": "dist/index.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["dist/**"] }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/index.js"
      }
  ]   
  
}